-- Print out the fibonacci numbers, computed with recursion
def fib(n: i32) -> i32 {
    let mut out = 0
    if n == 1 {
        out = 1
    } 
    else {
        if n >= 2 {
            out = fib(n - 1) + fib(n - 2)
        }
    }
    return out
}

let mut n = 0
while n < 13 {
    let result = fib(n)
    println(n)
    println(result)

    n = n + 1
}

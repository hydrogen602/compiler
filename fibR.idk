-- Print out the fibonacci numbers, computed with recursion
def fib(n: i32) -> i32 {
    return if n == 1 {
        1
    } 
    else {
        if n >= 2 {
            fib(n - 1) + fib(n - 2)
        }
        else {
            0
        }
    }
}

let mut n = 0
while n < 13 {
    let result = fib(n)
    println(n)
    println(result)

    n = n + 1
}


